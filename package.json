    "@babel/plugin-proposal-pipeline-operator": "^7.0.0-beta.47",
    "@babel/preset-env": "^7.0.0-beta.47",
    "@babel/preset-flow": "7.0.0-beta.47",
    "@babel/preset-react": "7.0.0-beta.47",
    "@babel/preset-stage-0": "7.0.0-beta.47",
    "@babel/preset-typescript": "^7.0.0-beta.47",
    "@nlabs/preset-stage-0": "7.0.0-beta.47",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.2",
    "@types/react": "^16.3.13",
    "@types/webpack": "^4.4.0",
