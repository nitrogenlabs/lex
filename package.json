    "babel-loader": "8.0.0-beta.3",
    "babel-polyfill": "7.0.0-beta.3",
    "caniuse-lite": "^1.0.30000844",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
