{
  "name": "@nlabs/lex",
  "version": "1.5.3",
  "description": "Lex",
  "license": "MIT",
  "bin": {
    "lex": "./dist/lex.js"
  },
  "keywords": [
    "arkhamjs",
    "lex",
    "nitrogenlabs",
    "webpack"
  ],
  "author": {
    "name": "Giraldo Rosales",
    "email": "giraldo@nitrogenlabs.com",
    "url": "http://nitrogenlabs.com"
  },
  "repository": {
    "type": "git",
    "url": "nitrogenlabs/lex"
  },
  "homepage": "https://github.com/nitrogenlabs/lex",
  "bugs": {
    "url": "https://github.com/nitrogenlabs/lex/issues"
  },
  "scripts": {
    "build": "NODE_ENV=production yarn clean && yarn typeCheck && yarn compile",
    "clean": "rm -rf ./dist",
    "compile": "babel src --out-dir dist --extensions '.ts,.tsx' -s inline",
    "lint": "tslint -c ./tslint.json -p ./tsconfig.json './src/**/*.{ts,tsx}'",
    "prepublishOnly": "yarn build",
    "reset": "rm -rf node_modules yarn.lock && yarn",
    "test": "NODE_ENV=test jest",
    "typeCheck": "tsc",
    "update": "yarn upgrade-interactive --latest",
    "watch": "NODE_ENV=development yarn clean && yarn typeCheck && yarn compile -w"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0-beta.47",
    "@babel/core": "^7.0.0-beta.47",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.47",
