{
  "name": "@nlabs/lex",
  "version": "1.8.6",
  "description": "Lex",
  "license": "MIT",
  "bin": {
    "lex": "./dist/lex.js"
  },
  "keywords": [
    "arkhamjs",
    "lex",
    "nitrogenlabs",
    "webpack"
  ],
  "author": {
    "name": "Giraldo Rosales",
    "email": "giraldo@nitrogenlabs.com",
    "url": "http://nitrogenlabs.com"
  },
  "repository": {
    "type": "git",
    "url": "nitrogenlabs/lex"
  },
  "homepage": "https://github.com/nitrogenlabs/lex",
  "bugs": {
    "url": "https://github.com/nitrogenlabs/lex/issues"
  },
  "scripts": {
    "build": "NODE_ENV=production && yarn clean && yarn typeCheck && yarn compile",
    "clean": "rm -rf ./dist",
    "compile": "yarn env && babel src --out-dir dist --extensions '.ts,.tsx' -s inline",
    "env": "LEX_CONFIG='{\"useTypescript\":true}'",
    "lint": "eslint ./src --ext .ts,.tsx",
    "prepublishOnly": "yarn build",
    "reset": "rm -rf node_modules yarn.lock && yarn",
    "test": "yarn lint && NODE_ENV=test && yarn env && jest",
    "typeCheck": "tsc",
    "update": "yarn upgrade-interactive --latest",
    "watch": "NODE_ENV=development yarn clean && yarn typeCheck && yarn compile -w"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.49",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.49",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-flow": "7.0.0-beta.49",
    "@babel/preset-react": "7.0.0-beta.49",
    "@babel/preset-stage-0": "7.0.0-beta.49",
    "@babel/preset-typescript": "^7.0.0-beta.49",
    "@nlabs/preset-stage-0": "7.0.0-beta.49",
    "@types/jest": "^23.0.0",
    "@types/node": "^10.3.2",
    "@types/react": "^16.3.17",
    "@types/webpack": "^4.4.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.0.1",
    "babel-loader": "8.0.0-beta.3",
    "babel-polyfill": "7.0.0-beta.3",
    "caniuse-lite": "^1.0.30000851",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "commander": "^2.15.1",
    "compare-versions": "^3.3.0",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "cssnano": "3.10.0",
    "download-npm-package": "^3.1.12",
    "eslint": "^4.19.1",
    "exports-loader": "^0.7.0",
    "file-loader": "^1.1.11",
    "find-file-up": "^2.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "jest": "23.1.0",
    "json-d-ts": "^1.0.1",
    "latest-version": "^4.0.0",
    "lodash": "^4.17.10",
    "node-sass": "^4.9.0",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-custom-properties": "^7.0.0",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "postcss-nested": "^3.0.0",
    "postcss-simple-vars": "^4.1.0",
    "postcss-url": "^7.3.2",
    "regenerator-runtime": "^0.11.1",
    "resolve": "^1.7.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "semver": "^5.5.0",
    "source-map-loader": "^0.2.3",
    "source-map-support": "^0.5.6",
    "style-loader": "^0.21.0",
    "svg-spritemap-webpack-plugin": "^2.5.0",
    "tsconfig-paths": "^3.3.1",
    "typescript": "^2.9.1",
    "webpack": "4.12.0",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.1.4"
  },
  "devDependencies": {
    "arkhamjs": "^3.5.0",
    "eslint-config-styleguidejs": "^0.7.4",
    "react": "^16.4.0",
    "react-test-renderer": "^16.4.0"
  }
}
