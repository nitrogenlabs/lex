{
  "name": "@nlabs/lex",
  "version": "1.9.22",
  "description": "Lex",
  "license": "MIT",
  "bin": {
    "lex": "./dist/lex.js"
  },
  "keywords": [
    "arkhamjs",
    "lex",
    "nitrogenlabs",
    "webpack"
  ],
  "author": {
    "name": "Giraldo Rosales",
    "email": "giraldo@nitrogenlabs.com",
    "url": "http://nitrogenlabs.com"
  },
  "repository": {
    "type": "git",
    "url": "nitrogenlabs/lex"
  },
  "homepage": "https://github.com/nitrogenlabs/lex",
  "bugs": {
    "url": "https://github.com/nitrogenlabs/lex/issues"
  },
  "scripts": {
    "build": "NODE_ENV=production && yarn clean && yarn typeCheck && yarn compile",
    "clean": "rm -rf ./dist",
    "compile": "yarn env && babel src --out-dir dist --extensions '.ts,.tsx' -s inline",
    "env": "LEX_CONFIG='{\"useTypescript\":true}'",
    "lint": "eslint ./src --ext .ts,.tsx",
    "preinstall": "node ./preinstall.js",
    "prepublishOnly": "yarn build",
    "reset": "rm -rf node_modules yarn.lock",
    "test": "yarn lint && NODE_ENV=test && yarn env && jest",
    "typeCheck": "tsc",
    "update": "yarn upgrade-interactive --latest",
    "watch": "NODE_ENV=development yarn clean && yarn typeCheck && yarn compile -w"
  },
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0-beta.52",
    "@babel/core": "^7.0.0-beta.52",
    "@babel/helper-plugin-utils": "^7.0.0-beta.52",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.52",
    "@babel/plugin-transform-classes": "^7.0.0-beta.52",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.52",
    "@babel/polyfill": "^7.0.0-beta.52",
    "@babel/preset-env": "^7.0.0-beta.52",
    "@babel/preset-flow": "7.0.0-beta.52",
    "@babel/preset-react": "7.0.0-beta.52",
    "@babel/preset-stage-0": "7.0.0-beta.52",
    "@babel/preset-typescript": "^7.0.0-beta.52",
    "@types/jest": "^23.1.5",
    "@types/node": "^10.5.2",
    "@types/react": "^16.4.6",
    "@types/webpack": "^4.4.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "8.0.0-beta.3",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-preset-jest": "^23.2.0",
    "caniuse-lite": "^1.0.30000865",
    "chalk": "^2.4.1",
