{
  "name": "@nlabs/favicons-webpack-plugin",
  "version": "1.15.1",
  "description": "Let webpack generate all your favicons and icons for you",
  "main": "./lib/index.js",
  "files": [
    "index.js",
    "lib/"
  ],
  "scripts": {
    "build": "NODE_ENV=production && rm -rf dist && yarn compile",
    "clean": "rm -rf dist node_modules yarn.lock package-lock.json *.log coverage",
    "compile": "tsc -p tsconfig.build.json",
    "lint": "eslint ./src --ext .ts,.tsx",
    "prepublishOnly": "yarn build",
    "reset": "rm -rf node_modules yarn.lock",
    "test": "yarn lint && NODE_ENV=test && jest",
    "update": "yarn upgrade-interactive --latest",
    "watch": "NODE_ENV=development rm -rf dist && yarn compile -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nitrogenlabs/favicons-webpack-plugin"
  },
  "keywords": [
    "webpack",
    "plugin",
    "html-webpack-plugin",
    "favicon",
    "icon"
  ],
  "author": {
    "name": "Jan Nicklas",
    "email": "j.nicklas@me.com",
    "url": "https://github.com/jantimon"
  },
  "contributors": [
    {
      "name": "Giraldo Rosales",
      "email": "giraldo@nitrogenlabs.com",
      "url": "https://nitrogenlabs.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nitrogenlabs/favicons-webpack-plugin/issues"
  },
  "homepage": "https://github.com/nitrogenlabs/favicons-webpack-plugin",
  "dependencies": {
    "eval": "^0.1.2",
    "favicons": "^5.2.0",
    "loader-utils": "^1.1.0",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@types/denodeify": "^1.2.31",
    "@types/favicons": "^5.1.0",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/jest": "^23.3.9",
    "@types/loader-utils": "^1.1.3",
    "@types/memory-fs": "^0.3.1",
    "@types/node": "^10.12.3",
    "@types/rimraf": "^2.0.2",
    "@types/webpack": "^4.4.18",
    "babel-loader": "^8.0.4",
    "denodeify": "^1.2.1",
    "dir-compare": "^1.0.1",
    "es6-promisify": "^6.0.1",
    "eslint": "^5.8.0",
    "eslint-config-styleguidejs": "^0.7.13",
    "express": "^4.16.4",
    "html-webpack-plugin": "^3.2.0",
    "memory-fs": "^0.4.1",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.5.2",
    "typescript": "^3.1.6",
    "webpack": "^4.25.1",
    "webpack-dev-middleware": "^3.4.0"
  },
  "peerDependencies": {
    "webpack": "^4.0.0"
  },
  "gitHead": "fd2d203d9345baad810ef261a27bb0a96009868d"
}
