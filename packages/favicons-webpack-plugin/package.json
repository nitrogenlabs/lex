{
  "name": "@nlabs/favicons-webpack-plugin",
  "version": "0.1.0",
  "description": "Let webpack generate all your favicons and icons for you",
  "main": "./lib/index.js",
  "files": [
    "index.js",
    "lib/"
  ],
  "scripts": {
    "build": "NODE_ENV=production && yarn clean && yarn compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "lint": "eslint ./src --ext .ts,.tsx",
    "prepublishOnly": "yarn build",
    "reset": "rm -rf node_modules yarn.lock",
    "test": "jest --clearCache && yarn lint && NODE_ENV=test && jest",
    "update": "yarn upgrade-interactive --latest",
    "watch": "NODE_ENV=development yarn clean && yarn compile -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nitrogenlabs/favicons-webpack-plugin"
  },
  "keywords": [
    "webpack",
    "plugin",
    "html-webpack-plugin",
    "favicon",
    "icon"
  ],
  "author": {
    "name": "Jan Nicklas",
    "email": "j.nicklas@me.com",
    "url": "https://github.com/jantimon"
  },
  "contributors": [
    {
      "name": "Giraldo Rosales",
      "email": "giraldo@nitrogenlabs.com",
      "url": "https://nitrogenlabs.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nitrogenlabs/favicons-webpack-plugin/issues"
  },
  "homepage": "https://github.com/nitrogenlabs/favicons-webpack-plugin",
  "dependencies": {
    "favicons": "^5.2.0",
    "loader-utils": "^1.1.0",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@types/denodeify": "^1.2.31",
    "@types/favicons": "^5.1.0",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/jest": "^23.3.2",
    "@types/loader-utils": "^1.1.3",
    "@types/memory-fs": "^0.3.1",
    "@types/node": "^10.10.1",
    "@types/rimraf": "^2.0.2",
    "@types/webpack": "^4.4.11",
    "babel-loader": "^8.0.2",
    "denodeify": "^1.2.1",
    "dir-compare": "^1.0.1",
    "es6-promisify": "^6.0.0",
    "eslint": "^5.6.0",
    "eslint-config-styleguidejs": "^0.7.9",
    "express": "^4.13.4",
    "html-webpack-plugin": "^3.2.0",
    "memory-fs": "^0.4.1",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.5.2",
    "typescript": "^3.0.3",
    "webpack": "^4.18.1",
    "webpack-dev-middleware": "^3.3.0"
  },
  "peerDependencies": {
    "webpack": "^4.0.0"
  }
}