  "description": "Lex",
  "license": "MIT",
  "type": "module",
  "bin": "./dist/lex.js",
  "keywords": [
    "arkhamjs",
    "lex",
    "nitrogenlabs",
    "webpack"
  ],
  "author": {
    "name": "Giraldo Rosales",
    "email": "giraldo@nitrogenlabs.com",
