{
  "extends": [
    "styleguidejs/typescript"
  ],
  "rules": {
    "import/order": [
      "error",
      {
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "index"
        ],
        "pathGroups": [
          {
            "pattern": "#!",
            "group": "builtin",
            "position": "before"
          },
          {
            "pattern": "/**/*|//*",
            "group": "builtin",
            "position": "after"
          },
          {
            "pattern": "type *",
            "group": "index",
            "position": "after"
          }
        ],
        "newlines-between": "always",
        "alphabetize": {
          "order": "asc",
          "caseInsensitive": true
        }
      }
    ],
    "import/extensions": 0,
    "jest/valid-expect": 0,
    "no-useless-escape": 0
  }
}
