{
  "name": "@nlabs/execa-mock",
  "version": "1.0.0",
  "description": "Execa jest mock",
  "license": "MIT",
  "bin": {
    "lex": "./dist/index.js"
  },
  "keywords": [
    "execa",
    "lex",
    "nitrogenlabs"
  ],
  "author": {
    "name": "Giraldo Rosales",
    "email": "giraldo@nitrogenlabs.com",
    "url": "http://nitrogenlabs.com"
  },
  "repository": {
    "type": "git",
    "url": "nitrogenlabs/lex"
  },
  "homepage": "https://github.com/nitrogenlabs/lex",
  "bugs": {
    "url": "https://github.com/nitrogenlabs/lex/issues"
  },
  "scripts": {
    "build": "NODE_ENV=production && rm -rf dist && yarn compile",
    "clean": "rm -rf dist node_modules yarn.lock package-lock.json *.log coverage",
    "compile": "tsc -p tsconfig.build.json",
    "env": "LEX_CONFIG='{\"useTypescript\":true}'",
    "lint": "eslint ./src --ext .ts,.tsx",
    "prepublishOnly": "yarn build",
    "reset": "rm -rf node_modules yarn.lock",
    "test": "yarn lint && NODE_ENV=test && yarn env && jest",
    "update": "yarn upgrade-interactive --latest",
    "watch": "NODE_ENV=development rm -rf dist && yarn compile -w"
  },
  "dependencies": {
    "cross-spawn": "^6.0.5",
    "get-stream": "^4.1.0",
    "is-stream": "^1.1.0",
    "npm-run-path": "^2.0.0",
    "signal-exit": "^3.0.0",
    "strip-eof": "^2.0.0",
    "uv": "^1.0.7"
  },
  "devDependencies": {
    "@types/jest": "^23.3.11",
    "@types/node": "^10.12.18",
    "eslint": "^5.12.0",
    "eslint-config-styleguidejs": "^0.7.13",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.2"
  }
}
