    "unicode-regex": "^3.0.0",
    "unified": "^8.4.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.22",
    "@types/mem": "^4.2.0",
    "@types/minimist": "^1.2.0",
    "@types/node": "^12.12.6",
    "codecov": "^3.6.1",
    "cross-env": "^6.0.3",
    "jest": "^25.0.0",
    "json-d-ts": "^1.0.1",
    "mkdirp": "^0.5.1",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "webpack": "^4.41.2"
  }
