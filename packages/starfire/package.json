{
  "name": "@nlabs/starfire",
  "version": "0.2.0",
  "description": "Starfire keeps code clean with style",
  "license": "MIT",
  "bin": {
    "starfire": "./dist/starfire.js"
  },
  "keywords": [
    "eslint",
    "nitrogenlabs",
    "starfire"
  ],
  "author": {
    "name": "Giraldo Rosales",
    "email": "giraldo@nitrogenlabs.com",
    "url": "http://nitrogenlabs.com"
  },
  "repository": {
    "type": "git",
    "url": "nitrogenlabs/starfire"
  },
  "homepage": "https://github.com/nitrogenlabs/starfire",
  "bugs": {
    "url": "https://github.com/nitrogenlabs/lex/issues"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "NODE_ENV=production && rm -rf dist && yarn compile",
    "build:docs": "node ./scripts/build/build-docs.js",
    "clean": "lex clean",
    "compile": "tsc -p tsconfig.build.json",
    "lint": "eslint ./src --ext .ts,.tsx",
    "prepublishOnly": "npm run prepare:release",
    "prepare:release": "npm i && npm run build && npm test",
    "pretest": "npm run lint",
    "test": "lex test",
    "update": "lex update -i",
    "watch": "lex compile -t -w"
  },
  "dependencies": {
    "@babel/code-frame": "^7.5.5",
    "@glimmer/syntax": "^0.43.0",
    "babel-eslint": "10.0.3",
    "babylon": "^6.18.0",
    "camelcase": "^5.3.1",
    "chalk": "^2.4.2",
    "cjk-regex": "^2.0.0",
    "commander": "^4.0.0",
    "cosmiconfig": "^6.0.0",
    "dashify": "^2.0.0",
    "dedent": "^0.7.0",
    "diff": "^4.0.1",
    "editorconfig": "^0.15.3",
    "emoji-regex": "^8.0.0",
    "escape-string-regexp": "^2.0.0",
    "eslint": "^6.6.0",
    "eslint-config-styleguidejs": "1.0.16",
    "eslint-friendly-formatter": "^4.0.1",
    "esutils": "^2.0.3",
    "find-file-up": "2.0.1",
    "find-project-root": "^1.1.1",
    "get-stream": "^5.1.0",
    "globby": "^10.0.1",
    "graphql": "^14.5.8",
    "gray-matter": "^4.0.2",
    "html-tag-names": "^1.1.4",
    "ignore": "^5.1.4",
    "jest-docblock": "^25.0.0",
    "json-stable-stringify": "^1.0.1",
    "leven": "^3.1.0",
    "lodash": "^4.17.15",
    "mem": "^5.1.1",
    "minimatch": "^3.0.4",
    "parse5": "^5.1.1",
    "postcss-less": "^3.1.4",
    "postcss-media-query-parser": "^0.2.3",
    "postcss-scss": "^2.0.0",
    "postcss-selector-parser": "^6.0.2",
    "postcss-values-parser": "^3.0.5",
    "read-pkg-up": "^7.0.0",
    "remark-frontmatter": "^1.3.2",
    "remark-parse": "^7.0.1",
    "resolve": "^1.12.0",
