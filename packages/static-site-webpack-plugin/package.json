{
  "name": "@nlabs/static-site-webpack-plugin",
  "version": "1.22.2",
  "description": "Minimal, static site generator powered by webpack",
  "main": "./lib/index.js",
  "scripts": {
    "build": "NODE_ENV=production && rm -rf dist && npm run compile",
    "clean": "rm -rf dist node_modules yarn.lock package-lock.json *.log coverage",
    "compile": "tsc -p tsconfig.build.json",
    "lint": "eslint ./src --ext .ts,.tsx",
    "prepublishOnly": "npm run build",
    "reset": "rm -rf node_modules yarn.lock",
    "test": "npm run lint && NODE_ENV=test && jest",
    "update": "npm-check --update",
    "watch": "NODE_ENV=development rm -rf dist && npm run compile -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nitrogenlabs/webpack-plugin-static-site"
  },
  "keywords": [
    "webpack",
    "plugin",
    "static-site-plugin",
    "static",
    "site"
  ],
  "author": {
    "name": "Mark Dalgleis"
  },
  "contributors": [
    {
      "name": "Giraldo Rosales",
      "email": "giraldo@nitrogenlabs.com",
      "url": "https://nitrogenlabs.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nitrogenlabs/static-site-webpack-plugin/issues"
  },
  "homepage": "https://github.com/nitrogenlabs/static-site-webpack-plugin",
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "eval": "^0.1.0",
    "lodash": "^4.17.10",
    "url": "^0.11.0",
    "webpack-sources": "^1.3.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "compression-webpack-plugin": "^2.0.0",
    "ejs": "^2.3.4",
    "eslint": "^5.13.0",
    "eslint-config-styleguidejs": "^1.0.2",
    "glob": "^7.0.3",
    "rimraf": "^2.6.3",
    "typescript": "^3.3.3",
    "webpack": "^4.29.3",
    "webpack-stats-plugin": "^0.2.1"
  },
  "peerDependencies": {
    "webpack": "^4.0.0"
  },
  "resolutions": {
    "webpack": "^4.0.0"
  },
  "gitHead": "fd2d203d9345baad810ef261a27bb0a96009868d"
}
