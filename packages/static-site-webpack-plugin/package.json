{
  "name": "@nlabs/static-site-webpack-plugin",
  "version": "0.1.1",
  "description": "Minimal, static site generator powered by webpack",
  "main": "./lib/index.js",
  "scripts": {
    "build": "yarn compile",
    "compile": "tsc -p tsconfig.build.json",
    "clean": "lex clean",
    "lint": "eslint ./src --ext .ts,.tsx",
    "prepublish": "yarn test",
    "test": "lex test",
    "update": "yarn upgrade-interactive --latest",
    "watch": "NODE_ENV=development yarn clean && yarn compile -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nitrogenlabs/webpack-plugin-static-site"
  },
  "keywords": [
    "webpack",
    "plugin",
    "static-site-plugin",
    "static",
    "site"
  ],
  "author": {
    "name": "Mark Dalgleis"
  },
  "contributors": [
    {
      "name": "Giraldo Rosales",
      "email": "giraldo@nitrogenlabs.com",
      "url": "https://nitrogenlabs.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nitrogenlabs/static-site-webpack-plugin/issues"
  },
  "homepage": "https://github.com/nitrogenlabs/static-site-webpack-plugin",
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "eval": "^0.1.0",
    "lodash": "^4.17.10",
    "url": "^0.11.0",
    "webpack-sources": "^1.3.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "compression-webpack-plugin": "^2.0.0",
    "ejs": "^2.3.4",
    "eslint": "^5.6.0",
    "eslint-config-styleguidejs": "^0.7.11",
    "glob": "^7.0.3",
    "rimraf": "^2.4.4",
    "typescript": "^3.0.1",
    "webpack": "^4.16.5",
    "webpack-stats-plugin": "^0.2.1"
  },
  "peerDependencies": {
    "webpack": "^4.0.0"
  }
}
