{
  "comments": false,
  "ignore": [
    "**/*.test.js",
    "**/*.test.ts",
    "**/*.test.tsx"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true
      }
    ]
  ],
  "presets": [
    "@babel/env",
    "@babel/typescript",
    [
      "@babel/stage-0",
      {
        "decoratorsLegacy": true,
        "pipelineProposal": "minimal"
      }
    ]
  ]
}